{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ANTLR4 Grammar",
      "type": "antlr-debug",
      "request": "launch",
      "input": "inputMiniLang.txt",
      "grammar": "MiniLang.g4",
      "printParseTree": true,
      "visualParseTree": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Ejecutar indexMiniLang.js",
      "program": "${workspaceFolder}/indexMiniLang.js",
      "console": "integratedTerminal"
    }
  ]
}